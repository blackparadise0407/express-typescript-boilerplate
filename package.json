{
  "name": "express-oop-base",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint --ext .ts --fix src/",
    "tsc": "tsc",
    "copy-assets": "ts-node-dev tools/copyAssets",
    "build": "concurrently \"yarn:clean\" \"yarn:lint\" \"yarn:tsc\" \"yarn:copy-assets\"",
    "build:dev": "concurrently \"yarn:clean\" \"yarn:tsc\" \"yarn:copy-assets\"",
    "start": "node .",
    "start:prod": "yarn build && yarn start",
    "start:dev": "yarn build:dev && yarn start",
    "dev": "nodemon",
    "prepare": "husky install",
    "test": "mocha -r ts-node-dev test/**/*.spec.ts"
  },
  "dependencies": {
    "bull": "^4.8.1",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/bull": "^3.15.8",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "_moduleAliases": {
    "@app": "dist/src"
  }
}
